# KRXUSD Backend Configuration

# Mock Mode - 프론트엔드 개발용 가짜 데이터 반환
# true: Mock 데이터 사용 (API 키 불필요)
# false: 실제 데이터 사용 (FinanceDataReader)
USE_MOCK=true

# CORS Origins
CORS_ORIGINS=["http://localhost:3000"]

# Database (선택)
DATABASE_URL=sqlite:///./krxusd.db

# Redis (선택)
REDIS_URL=redis://localhost:6379

# Debug mode
DEBUG=true
